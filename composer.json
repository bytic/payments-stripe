{
  "name": "bytic/payments-stripe",
  "type": "library",
  "license": "MIT",
  "description": "Payments module for bytic library for stripe",
  "keywords": [],
  "homepage": "https://github.com/bytic/payments-stripe",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/ByTIC/Common.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\Payments\\Stripe\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Payments\\Stripe\\Tests\\": "tests/src",
      "ByTIC\\Payments\\Stripe\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.1",
    "omnipay/stripe": "^2.0|^3.0",
    "stripe/stripe-php": "^7.0"
  },
  "require-dev": {
    "bytic/payments": "^1.0",
    "bytic/payments-tests": "^1.0",
    "bytic/form": "^1.0",
    "phpunit/phpunit": "^6.5|^7.5|^8.0|^9.0",
    "vlucas/phpdotenv": "^4.0",
    "omnipay/tests": "^3"
  },
  "suggest": {
    "bytic/facebook-pixel": "^1.0"
  }
}
